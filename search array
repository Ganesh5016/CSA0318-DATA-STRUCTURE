#include<stdio.h>
main()
{
  int a[10],n,i;
   printf("enter the size of array :: \n");
  scanf("%d",&n);
printf("enter the elements :: \n");
  for(i=1;i<=n;i++)
  {
    scanf("%d",&a[i]);
  }
  int k;
  printf("enter the number to be searched :: \n");
  scanf("%d",&k);
  for(i=k;i<=n;i++)
  {
    if(k==a[i])
    {
    	printf("%d",i);
	}
  }
 
}
